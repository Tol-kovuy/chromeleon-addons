{
  "manifest_version": 3,
  "name": "Chromeleon Auto Proxy",
  "version": "1.0.1",
  "permissions": [
    "storage",  
    "proxy",
    "tabs",
    "webRequest",
    "webRequestAuthProvider",
    "storage"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}
